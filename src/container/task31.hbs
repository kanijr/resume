<main>
    <div class="row g-4"></div>
    {{#with navigation}}
    <section class="col-12">
        {{#each links}}
        <a href="{{href}}" class="btn btn-link">{{text}}</a>

        {{/each}}

        {{#with button}}
        <a href="{{href}}" class="btn btn-primary">{{text}}</a>
        {{/with}}
    </section>
    {{/with}}

    <hr>
    <br>

    {{#with header}}
    <section class="col-12">
        <h1>{{title}}</h1>
        <p>{{description}}</p>
        {{#with button}}
        <a href="{{link}}" class="btn btn-primary">{{text}}</a>
        {{/with}}
    </section>
    {{/with}}

    <br>
    <hr>


    {{#with city}}
    <section class="col-12">
        <h2>{{title}}</h2>
        <br>
        <div class="row g-4">
            {{#each cards}}
            <div class="col-6">
                <div class="card">
                    {{#with picture}}
                    <img src="{{src}}" alt="{{alt}}">
                    {{/with}}
                    <div class="card-body">
                        <div class="card-title">{{title}}</div>
                        <p>{{description}}</p>
                        {{#with button}}
                        <a href="{{href}}" class="btn btn-primary">{{text}}</a>
                        {{/with}}
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
    {{/with}}

    <br>
    <hr>
    {{#with winner}}
    <section class="col-12">
        <h2>{{title}}</h2>
        <br>
        {{#with table}}
        <table class="table">
            <thead>
                <tr>
                    {{#each head}}
                    <th>{{this}}</th>
                    {{/each}}
                </tr>
            </thead>
            <tbody>
                {{#each user}}
                <tr>
                    <th>{{place}}</th>
                    <td>{{name}}</td>
                    <td>{{surname}}</td>
                    <td>{{nickname}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        {{/with}}
    </section>
    {{/with}}

    <br>
    <hr>

    {{#with gallery}}
    <section class="col-12">
        <h2>{{title}}</h2>
        <br>
        <div class="row g-1">
            {{#each cards}}
            <div class="col-4">
                <div class="card">
                    <div class="card-header">{{name}}</div>
                    <ul class="list-group list-group-flush">
                        {{#each items}}
                        <li class="list-group-item">{{this}}</li>
                        {{/each}}
                    </ul>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
    {{/with}}

    <br>
    <hr>

    {{#with relaxion}}
    <section class="col-10">
        <div class="display-3">{{title}}</div>
        <br>
        <div class="row g-4">
            {{#each cards}}
            <div class="col-5">
                <div class="card">
                    <div class="card-header text-bg-secondary">{{header}}</div>
                    <div class="card-body text-bg-secondary">
                        <div class="card-title">{{title}}</div>
                        <p>{{description}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
    {{/with}}

    <br>
    <hr>

    {{#with rest}}
    <section class="col-12">
        <div class="row g-4">
            {{#each cards}}
            <div class="col-5">
                <div class="card text-center">
                    <div class="card-header">{{header}}</div>
                    <div class="card-body">
                        <div class="card-title">{{title}}</div>
                        <p>{{description}}</p>
                        <button class="btn btn-primary">{{button}}</button>
                    </div>
                    <div class="card-footer">{{footer}}</div>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
    {{/with}}

    <br>
    <hr>

    {{#with proposition}}
    <section class="col-10">
        <div class="display-2">{{title}}</div>
        <br>
        <div class="row g-4">
            {{#each card}}
            <div class="col-5">
                <div class="card">
                    <div class="card-header text-bg-warning text-light">{{header}}</div>
                    <div class="card-body text-bg-warning text-light">
                        <div class="card-title">{{title}}</div>
                        <p>{{description}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
    {{/with}}
</main>
<br>
<hr>
{{#with footer}}
<footer>
    <h5>{{title}}</h5>
    <br>
    <div class="row gx-5 gy-2">
        <div class="col-3">
            {{#each navigation}}
            <a href="{{href}}" class="btn btn-link">{{text}}</a>
            <br>
            {{/each}}
        </div>
        <div class="col-3">
            {{#each socialMedia}}
            <a href="{{link}}" class="btn btn-outline-dark">{{name}}</a>
            <br>
            {{/each}}
        </div>
        <div class="col-3">
            {{#each partners}}
            <a href="{{link}}" class="btn btn-outline-info">{{name}}</a>
            <br>
            {{/each}}
        </div>
    </div>
</footer>
{{/with}}