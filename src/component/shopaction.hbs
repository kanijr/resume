<section>
    {{#with service}}
    <div class="card text-bg-success text-center">
        <div class="card-body">
            <div class="card-title">
                <h2>{{title}}</h2>
            </div>
            <p>{{description}}</p>
            {{#each buttons}}
            <a class="btn btn-primary" href="{{link}}">{{text}}</a>
            {{/each}}
        </div>
    </div>
    {{/with}}
</section>